{
  "name": "skill-badges",
  "version": "1.0.0",
  "description": "Generate SVG skill badges using Simple Icons",
  "main": "generate.js",
  "type": "module",
  "scripts": {
    "build": "node generate.js",
    "build:limited": "node generate.js --limit=100",
    "dev": "vercel dev",
    "deploy": "vercel --prod",
    "clean": "rimraf public/badges/*.svg",
    "test": "node generate.js --limit=10"
  },
  "dependencies": {
    "simple-icons": "^13.0.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.0"
  },
  "keywords": [
    "badges",
    "svg",
    "simple-icons",
    "skills"
  ],
  "author": "",
  "license": "MIT"
}
